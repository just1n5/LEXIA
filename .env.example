# Variables de entorno para desarrollo local
# Copiar como .env y configurar valores reales

# === BASE DE DATOS ===
POSTGRES_DB=consulta_judicial
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here

# === RABBITMQ ===
RABBITMQ_USER=admin
RABBITMQ_PASS=your_rabbitmq_password_here

# === BACKEND API ===
SECRET_KEY=your_super_secret_key_here_minimum_32_characters
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# === EMAIL SERVICIO ===
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_email_password_here

# === RPA CONFIGURACIÃ“N ===
RAMA_JUDICIAL_URL=https://consultaprocesos.ramajudicial.gov.co/Procesos/NumeroRadicacion
HEADLESS_BROWSER=true
SELENIUM_TIMEOUT=30

# === ENTORNO ===
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO
