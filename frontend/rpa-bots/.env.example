# === CONFIGURACIÓN DE BASE DE DATOS ===
DATABASE_URL=postgresql://postgres:password@localhost:5432/consulta_judicial

# === CONFIGURACIÓN DE CELERY ===
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# === CONFIGURACIÓN DE RPA ===
RAMA_JUDICIAL_URL=https://consultaprocesos.ramajudicial.gov.co/Procesos/NumeroRadicacion
HEADLESS_BROWSER=true
SELENIUM_TIMEOUT=30
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY=5

# === CONFIGURACIÓN DE LOGS ===
LOG_LEVEL=INFO
LOG_FILE_PATH=./logs/rpa_worker.log

# === CONFIGURACIÓN DE ENTORNO ===
ENVIRONMENT=development
DEBUG=true
